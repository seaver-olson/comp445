{
    "version": 5,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 24,
        "patch": 0
    },
    "include": [
        "CMakePresets_common.json"
    ],
    "configurePresets": [
        {
            "name": "coordinator-rdrw612bga",
