{
    "version": 5,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 24,
        "patch": 0
    },
    "include": [
        "CMakePresets_common.json"
    ],
    "configurePresets": [
        {
            "name": "coordinator-frdmrw612",
            "displayName": "Zigbee Coordinator FRDM-RW612",
            "description": "Build Zigbee Coordinator on FRDM-RW612 board",
            "inherits": [
                "coordinator",
                "frdmrw612"
            ],
            "hidden": false
        },
        {
            "name": "coordinator-r23-frdmrw612",
            "displayName": "Zigbee Coordinator R23 FRDM-RW612",
            "description": "Build Zigbee Coordinator R23 on FRDM-RW612 board",
            "inherits": [
                "coordinator-r23",
                "frdmrw612"
            ],
            "hidden": false
        }
    ],
    "buildPresets": [
        {
            "name": "coordinator-frdmrw612-debug",
            "description": "Build only the Debug configuration",
            "displayName": "Debug",
            "configurePreset": "coordinator-frdmrw612",
            "configuration": "Debug"
        },
        {
            "name": "coordinator-frdmrw612-minsizerel",
            "description": "Build only the MinSizeRel configuration",
            "displayName": "MinSizeRel",
            "configurePreset": "coordinator-frdmrw612",
            "configuration": "MinSizeRel"
        },
        {
            "name": "coordinator-r23-frdmrw612-debug",
            "description": "Build only the Debug configuration",
            "displayName": "Debug",
            "configurePreset": "coordinator-r23-frdmrw612",
            "configuration": "Debug"
        },
        {
            "name": "coordinator-r23-frdmrw612-minsizerel",
            "description": "Build only the MinSizeRel configuration",
            "displayName": "MinSizeRel",
            "configurePreset": "coordinator-r23-frdmrw612",
            "configuration": "MinSizeRel"
        }
    ]
}